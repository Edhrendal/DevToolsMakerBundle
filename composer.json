{
    "name": "edhrendal/dev-tools-maker-bundle",
    "description": "Edhrendal dev tools upon Symfony/MakerBundle",
    "keywords": ["symfony", "bundle", "maker", "dev", "tools"],
    "homepage": "https://github.com/Edhrendal/DevToolsMakerBundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Edhrendal\\DevToolsMakerBundle\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/options-resolver": "^6.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0"
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "@rm-composer-lock"
        ],
        "post-update-cmd": [
            "@rm-composer-lock"
        ],
        "rm-composer-lock": "rm composer.lock"
    }
}
